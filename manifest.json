{
  "name": "travian",
  "version": "1.0",
  "description": "auto play travian",
  "permissions": ["tabs","storage","<all_urls>", "http://*/*", "https://*/*"],
  "browser_action": {  
        "default_icon": "get_started128.png"
  },
  "content_scripts": [
      {
          "matches": ["https://t4x.crusadertrav.eu/dorf1.php*"],
          "js": ["content_script.js"]
      }
      ,
      {
          "matches": ["https://t4x.crusadertrav.eu/build.php?id*"],
          "js": ["content_script2.js"]
      },
      {
          "matches": ["https://t4x.crusadertrav.eu/dorf2.php*"],
          "js": ["dorf2.js"]
      }
  ],


  "manifest_version": 2
}
